<%= render "shared/header"%>


<div class="question-index-page">
  <h1>質問一覧</h1>

  <% @questions.each do |question|%>
  <div class="question-card">
    <div class="question-card-genre"><%= question.genre.name%></div>
    <div class="question-card-title"><%= link_to "#{question.title}", question_path(question.id)%></div>
  </div>
  <%end%>

</div>